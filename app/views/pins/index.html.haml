#pins.transitions-enabled
  - @pins.each do |pin|
    .box.panel.panel-default
      = link_to (image_tag pin.image.url), pin
      .panel-body
        %h2= link_to pin.title, pin
        %p.user
        Submitted by
        = pin.user.name
      .panel-footer
        Tags:
        - pin.tags.each do |tag|
          = link_to tag.name, tag_path(tag.name) 

     